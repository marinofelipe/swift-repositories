platform :ios, '9.0'

target 'SwiftRepositories' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Alamofire'
  pod 'AlamofireImage'
  pod 'SwiftMessages'

  target 'SwiftRepositoriesTests' do
    inherit! :search_paths
    pod 'OHHTTPStubs/Swift'
  end

end
